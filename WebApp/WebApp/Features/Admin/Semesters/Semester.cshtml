@model WebApp.Models.Semester
<div class="page-content">
    <h1 class="page-title">
        @Model.season @Model.year
    </h1>
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <i class="fa fa-home"></i>
                <a href="/Admin">Dashboard</a>
                <i class="fa fa-angle-right"></i>
            </li>
            <li>
                <a href="/Admin/Semesters">Semesters</a>
                <i class="fa fa-angle-right"></i>
            </li>
            <li>
                @Model.season @Model.year (Semester ID# @Model.Id)
            </li>
        </ul>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="portlet light bordered" id="">
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="control-label col-md-3">Season</label>
                                <div class="col-md-9">
                                    <p class="form-control-static">@Model.season</p>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3">Year</label>
                                <div class="col-md-9">
                                    <p class="form-control-static">@Model.year</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>