@model WebApp.Models.Professor
<div class="page-content">
    <h1 class="page-title">
        @Model.firstname @Model.lastname
    </h1>
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <i class="fa fa-home"></i>
                <a href="/Admin">Dashboard</a>
                <i class="fa fa-angle-right"></i>
            </li>
            <li>
                <a href="/Admin/Professors">Professors</a>
                <i class="fa fa-angle-right"></i>
            </li>
            <li>
                @Model.firstname @Model.lastname (Professor ID# @Model.Id)
            </li>
        </ul>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="portlet light bordered" id="">
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="control-label col-md-3">First Name</label>
                                <div class="col-md-9">
                                    <p class="form-control-static">@Model.firstname</p>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3">Last Login</label>
                                <div class="col-md-9">
                                    <p class="form-control-static">@Model.lastlogin</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="control-label col-md-3">Last Name</label>
                                <div class="col-md-9">
                                    <p class="form-control-static">@Model.lastname</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>