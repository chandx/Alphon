@model WebApp.Features.Student.MyCourses.MyCoursesController
<div class="page-content">
    <h1 class="page-title">
        My Courses
    </h1>
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <i class="fa fa-home"></i>
                <a href="/Student">Dashboard</a>
                <i class="fa fa-angle-right"></i>
            </li>
            <li>
                My Courses
            </li>
        </ul>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="portlet light bordered" id="">
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>
                                    <h4>My Courses</h4>
                                </label>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="control-label col-md-3">Semester:</label>
                                <div class="col-md-3">
                                    <p class="form-control-static">Fall 2017</p>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-striped table-bordered table-hover table-checkable order-column" id="courseList">
                                    <thead>
                                        <tr>
                                            <th> Room </th>
                                            <th> Times </th>
                                            <th> Professor </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <th><a href="">Course1</a></th>
                                            <th> Monday 4pm / Tuesday 4pm </th>
                                            <th> Prof Name </th>
                                        </tr>
                                        <tr>
                                            <th><a href="">Course2</a></th>
                                            <th> Wednesday 4pm / Thursday 4pm </th>
                                            <th> Prof Name </th>
                                        </tr>
                                        <tr>
                                        <tr>
                                            <th><a href="">Course3</a></th>
                                            <th> Friday 4pm </th>
                                            <th> Prof Name </th>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>